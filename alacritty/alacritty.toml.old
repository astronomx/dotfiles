[colors.bright]
black = "#686868"
blue = "#5CCFE6"
cyan = "#95E6CB"
green = "#A6CC70"
magenta = "#FFEE99"
red = "#F27983"
white = "#FFFFFF"
yellow = "#FFCC66"

[colors.normal]
black = "#191E2A"
blue = "#73D0FF"
cyan = "#95E6CB"
green = "#BAE67E"
magenta = "#FFD580"
red = "#FF3333"
white = "#C7C7C7"
yellow = "#FFA759"

[colors.primary]
background = "#282828"
foreground = "#54ecc4"

[font]
size = 12.0

[font.normal]
family = "ProggyClean Nerd Font"

[shell]
args = ["-l", "-c", "tmux && clear"]
program = "/bin/zsh"

[window]
opacity = 0.5

decorations = "none"
[window.dimensions]
columns = 88
lines = 24

[window.padding]
x = 5
y = 5

[window.position]
x = 1600
y = 800
